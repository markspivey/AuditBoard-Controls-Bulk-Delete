# AuditBoard API Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# AuditBoard Base URL (your organization's AuditBoard instance)
# Example: https://your-org.auditboardapp.com/api/v1
# Use sandbox for testing: https://your-org.auditboardsandbox.com/api/v1
AUDITBOARD_BASE_URL=https://your-org.auditboardapp.com/api/v1

# AuditBoard API Token
# Get this from your AuditBoard instance under Settings â†’ API Tokens
AUDITBOARD_API_TOKEN=your_token_here

# Safety Settings
# Set to 'false' to enable live deletion (USE WITH EXTREME CAUTION!)
DRY_RUN=true

# Logging Configuration
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Directory for log files and results
LOG_DIR=results
RESULTS_DIR=results
